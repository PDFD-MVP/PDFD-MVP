@model IEnumerable<VisitorLog_PDFD.Models.Report>

@{
    ViewBag.Title = "Report";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container mt-5">
    <h1 class="text-center">Report</h1>
    <table class="table table-bordered table-striped mt-4">
        <thead>
            <tr>
                <th>Person Name</th>
                <th>Continent</th>
                <th>Country</th>
                <th>State</th>
                <th>County</th>
                <th>City</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var report in Model)
            {
                    <tr>
                        <td>@report.PersonName</td>
                        <td>@report.ContinentName</td>
                        <td>@report.CountryName</td>
                        <td>@report.StateName</td>
                        <td>@report.CountyName</td>
                        <td>@report.CityName</td>
                    </tr>
            }
        </tbody>
    </table>
</div>
